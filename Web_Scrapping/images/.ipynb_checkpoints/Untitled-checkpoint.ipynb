   "source": [
    "def fibo(x):\n",
    "    prev, pos = 0, 1\n",
    "    for i in range(x, 0, -1):\n",
    "        pos += prev\n",
    "        prev = pos-prev\n",
    "    print(pos)"
   ]
  },
